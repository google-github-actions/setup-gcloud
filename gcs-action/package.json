{
  "name": "gcs-action",
  "version": "0.0.1",
  "private": true,
  "description": "GCS action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoogleCloudPlatform/github-actions/setup-gcloud.git"
  },
  "keywords": [
    "actions",
    "gcs",
    "setup"
  ],
  "author": "GoogleCloudPlatform",
  "license": "Apache 2.0",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.3",
    "@google-cloud/storage": "^4.3.1",
    "@types/js-base64": "^2.3.1",
    "@types/tmp": "^0.1.0",
    "js-base64": "^2.5.2",
    "tmp": "^0.1.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.27",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
}
