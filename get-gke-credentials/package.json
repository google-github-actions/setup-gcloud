{
  "name": "get-gke-credentials",
  "version": "0.1.0",
  "description": "Get short lived credentials to a GKE cluster",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/main.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "fixlint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write **/*.ts",
    "test": "mocha -r ts-node/register -t 600s 'tests/*.test.ts'",
    "e2e-tests": "mocha -r ts-node/register -t 120s 'tests/e2e/e2e.test.ts'",
    "cleanup": "mocha -r ts-node/register -t 240s 'tests/cleaner/cleanTestFunctions.test.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GoogleCloudPlatform/github-actions"
  },
  "keywords": [
    "gke",
    "kubernetes",
    "GCP",
    "google cloud",
    "Google Kubernetes Engine"
  ],
  "author": "GoogleCloudPlatform",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "fs": "0.0.1-security",
    "google-auth-library": "^6.0.6",
    "uuidv4": "^6.2.3"
  },
  "devDependencies": {
    "@kubernetes/client-node": "^0.12.2",
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/lodash": "^4.14.159",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.14.5",
    "@types/sinon": "^9.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "@zeit/ncc": "^0.22.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^8.1.1",
    "prettier": "^2.0.5",
    "sinon": "^9.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "yaml": "^1.10.0"
  }
}
